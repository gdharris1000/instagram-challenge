<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<% if logged_in? %>
    <h2>Welcome <%= current_user.name %></h2>
    <%else%>
    <%= form_tag '/login' do%>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <%= submit_tag "Login" %>
<% end %>
<% end %>
<%= button_to "Sign Up", "/users/new", method: :get %>